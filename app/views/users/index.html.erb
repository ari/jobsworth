<% @page_title = "Users - #{Setting.productName}" %>

<legend>
  Users
  <%= link_to _('New User'), new_user_path, :class => "pull-right" %>
</legend>

<% for user in @users %>
  <%= render :partial => "user", :locals => {:user => user} %>
<% end %>

<%= will_paginate(@users, { :next_label => _('Next') + ' &raquo;'.html_safe, :prev_label => '&laquo; '.html_safe + _('Previous') }) %>

