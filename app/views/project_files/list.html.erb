<table width="100%">
    <tr>
      <td align=left class="page_header">
         <div style="float:left;"><%=_ 'Files' %> - <%= @current_folder.full_path %></div>
         <div style="float:right;padding-top:0.1em"> 
           <%= link_to_function _('New File'), "new_project_file('file', #{@current_folder.try(:id) || "''"})", :id => "new_project_file" %>
           /
           <%= link_to_function _('New Folder'), "new_project_file('folder', #{@current_folder.try(:id) || "''"})", :id => "new_project_folder" %>
           </div>
       </td>
     </tr>
<tr><td ><div id="inline_form" style="display:none;"></div>
  <div><%=flash[:notice]%></div>
</td></tr>
  <tr>
    <td id="files">
<% for folder in @project_folders %>
  <%= render :partial => 'folder_cell', :locals => {:folder => folder } %>
<% end %>
<div id="dir_sep" style="display:none; float:left;">&nbsp;</div>
<% if @project_files
  for project_files in @project_files
%>
    <%= render :partial => 'file_cell', :locals => {:project_files => project_files}%>
<% end %>
<% end %>
    </td>
  </tr>
  <tr><td colspan=6>&nbsp;</td></tr>
  <tr><td colspan=6></td></tr>
  </table>


